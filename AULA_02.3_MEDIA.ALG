algoritmo "AULA_02.3_MEDIA"
var
   numero1, numero2: Inteiro
   media: Real
inicio
      Escreva("Digite o 1º numero: ")
      Leia(numero1)
      Escreva("Digite o 2º numero: ")
      Leia(numero2)
      media <- (numero1 + numero2) / 2
      Escreva("A media entre ", numero1, " e ", numero2, " e igual a ", media)
fimalgoritmo