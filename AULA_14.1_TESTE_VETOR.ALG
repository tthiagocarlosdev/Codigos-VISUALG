algoritmo "AULA_14.1_TESTE_VETOR"
var
   V: vetor[1..6] de Inteiro
   Contador: Inteiro
inicio
      Para Contador <- 1 ate 6 faca
           Escreva("Digite o ", Contador, "° valor: ")
           Leia(V[contador])
      FimPara
      
      Para Contador <- 1 ate 6 faca
           Escreva("{", V[Contador], "}")
      FimPara
fimalgoritmo